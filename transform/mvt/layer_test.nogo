package mvt

import (
	"encoding/json"
	"fmt"
	"github.com/buckhx/diglet/transform/mvt/vector_tile"
	"io/ioutil"
	"testing"
)

func TestMarshal(t *testing.T) {
	data, err := ioutil.ReadFile("../test_data/12_2124_1373.pbf.gz")
	if err != nil {
		panic(err)
	}
	vtile, err := vector_tile.DecodeGzipped(data)
	if err != nil {
		panic(err)
	}
	bytes, err := json.MarshalIndent(vtile, "", "\t")
	fmt.Println(string(bytes))
}
